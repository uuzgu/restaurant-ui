{
  "rewrites": [
    { "source": "/payment/cancel", "destination": "/index.html" },
    { "source": "/payment/success", "destination": "/index.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/payment/cancel", "dest": "/index.html" },
    { "src": "/payment/success", "dest": "/index.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
} 